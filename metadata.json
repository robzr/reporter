{
  "name": "robzr-reporter",
  "version": "0.1.1",
  "author": "Rob Zwissler",
  "license": "MIT",
  "summary": "Simplified recording & extracting of facts, system or ruby commands via Puppet reporting.",
  "source": "https://github.com/robzr/reporter",
  "project_page": "https://github.com/robzr/reporter",
  "issues_url": "https://github.com/robzr/reporter/issues",
  "tags": ["client", "logging", "server", "report", "reports", "reporting", "echo", "log"],
  "operatingsystem_support": [
    {
      "operatingsystem": "POSIX",
      "operatingsystemrelease": ["any"]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": ["5", "6", "7"]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": ["5", "6", "7"]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": ["6", "7"]
    },
    {
      "operatingsystem": "Scientific",
      "operatingsystemrelease": ["5", "6", "7"]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": ["6", "7"]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": ["8", "10", "12", "14", "16"]
    }
  ],
  "dependencies": [
    {
      "name":"puppetlabs-stdlib","version_requirement":">= 1.0.0"
    }
  ],
  "data_provider": null
}
